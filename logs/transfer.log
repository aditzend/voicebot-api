    labels:
      botfarm: duckling
    #ports:
     # - 3${STACK_CODE-00}62:8000
    networks:
      - public
    
networks:
  private:
  public:
  proxy:
    external: true
    name: ${PROXY_NET-traefik_net}

volumes:
